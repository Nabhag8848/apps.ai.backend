{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "plugins": [
    //plugins
    "plugin: @typescript-eslint / eslint-plugin",
    "plugin: @typescript-eslint / recommended"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-empty": "off",
    "no-irregular-whitespace": "off"
  }
}
